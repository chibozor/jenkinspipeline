pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('BUILD') {
            steps {
                echo 'Developpement'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing'
            }
        }
        stage('PRODUCTION') {
            steps {
                echo 'La mise en production'
            }
        }
    }
}
